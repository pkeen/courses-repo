		"types": "tsc --noEmit"
	},
	"dependencies": {
		"zod": "^3.0.0"
	},
	"peerDependencies": {
		"typescript": "^5.0.0"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^28.0.1",
		"@rollup/plugin-multi-entry": "^6.0.1",
		"@rollup/plugin-node-resolve": "^15.3.0",
