{
	"name": "@pete_keen/courses-core",
	"version": "0.0.1",
	"main": "dist/index.mjs",
	"browser": "dist/client/index.mjs",
	"types": "dist/index.d.ts",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.mjs",
			"default": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		},
		"./validators": {
			"import": "./dist/validators/index.mjs",
			"default": "./dist/validators/index.mjs",
			"types": "./dist/validators/index.d.ts"
		},
		"./entities": {
			"import": "./dist/entities/index.mjs",
			"default": "./dist/entities/index.mjs",
			"types": "./dist/entities/index.d.ts"
		},
		"./ports": {
			"import": "./dist/application/ports/index.mjs",
			"default": "./dist/application/ports/index.mjs",
			"types": "./dist/application/ports/index.d.ts"
		}
